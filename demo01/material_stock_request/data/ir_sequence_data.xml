<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="stock_request_code" model="ir.sequence">
                <field name="name">Transfer:Stock Request</field>
            <field name="code">stock_request_sequence_code</field>
             <field name="prefix">STR-</field>
            <field name="padding">2</field>
            <field name="number_next">1</field>
            <field name="suffix">-%(range_y)s</field>
            <field name="is_department">True</field>
            <field name="number_increment">1</field>
        </record>
        <record id="material_request_code" model="ir.sequence">
                <field name="name">Transfer:Material Request</field>
            <field name="code">material_request_sequence_code</field>
             <field name="prefix">MTQ-</field>
            <field name="padding">2</field>
            <field name="number_next">1</field>
            <field name="suffix">-%(range_y)s</field>
            <field name="is_department">True</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>

