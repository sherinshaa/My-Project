<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="lot_serial_warning_wizard_view_form" model="ir.ui.view">
        <field name="name">lot.serial.warning.wizard.view.form</field>
        <field name="model">transfer.warning</field>
        <field name="arch" type="xml">
            <form string="Test">
                <div>Validate the transfer with Lot/Serial number.</div>
                <footer>
                    <button string="Cancel" class="o_back_button btn-primary"
                            special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <record id="lot_serial_warning_action"
            model="ir.actions.act_window">
        <field name="name">Lot Serial Warning</field>
        <field name="res_model">transfer.warning</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>
</odoo>